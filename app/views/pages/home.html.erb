
<div class="jumbotron">
	<h1>Welcome to Pinteresting</h1>
	<% if user_signed_in? %>
          Welcome to the Members Area!
     <% else %>
		<p>
			<%= link_to "Log in", new_user_session_path, class:" btn btn-default btn-lg" %>
			<%= link_to "Sign Up", new_user_registration_path, class: "btn btn-primary btn-lg active" %>
		</p>
	<% end %>
</div>

<p>Alternatively you may <%= link_to "SIGN UP HERE", "#", class: "btn btn-danger btn-lg" %>.</p>
